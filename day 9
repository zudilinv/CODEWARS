//////////////////////////////////
//Вернуть курс йены с сотыми(1.00)
function usdcny(usd) {
 const curs = usd * 6.75
 return `${curs.toFixed(2)} Chinese Yuan`;
}
/////////////////////////////////
//Если это куб - true
var cubeChecker = function(volume, side){
  if (side <= 0) {
    return false;
  }else if (volume === side ** 3) {
    return true;
  }
  return false;
};
