//Обернуть строку более короткой
function solution(a, b) {
    if (a.length < b.length) {
        return a + b + a;
    } 
        return b + a + b;
    }
/////////////////////////////////
//
function createArray(number){
  var newArray = [];
  
  for(var counter = 1; counter <= number; counter++){
    newArray.push(counter);
  }
  
  return newArray;
}
//////////////////////////////////
