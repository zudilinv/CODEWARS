//Милисекунды
function past(h, m, s){
  return ((s * 1000) + (m * 60000) + (h * 3600000));
}
////////////////////////
//Удвоить целое число
function doubleInteger(i) {
  return i * 2;
}
////////////////////////
//Умножение всех значений в массиве по порядку
function grow(x){
  const xReс = x.reduce((acc, rec) => acc * rec);
 return xReс;
}
