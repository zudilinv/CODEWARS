//Строку в число
const stringToNumber = function(str){
  return +str;  // варианты: parseInt(str); Number(str);
  return null;
}
/////////////////////////////
//Вставить значение  в строку
function greet(name){
  return `Hello, ${name} how are you doing today?`; 
}
/////////////////////////////
//Если сумма нечетное число, то true
function lovefunc(flower1, flower2){
  if ((flower1 + flower2) % 2 !== 0) {
    return true;
  }
  return false;
}
/////////////////////////////
